# Disable webpack persistent caching for development
NEXT_WEBPACK_DISABLE_CACHE=1

# Increase memory limit and suppress deprecation warnings for Node.js
NODE_OPTIONS=--max-old-space-size=4096 --no-warnings --no-deprecation

# Disable telemetry
NEXT_TELEMETRY_DISABLED=1

# Disable React DevTools in development
REACT_DEVTOOLS_DISABLE=true 

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:5000